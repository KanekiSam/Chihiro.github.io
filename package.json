{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.5",
    "@umijs/hooks": "^1.8.1",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.14",
    "antd": "^4.1.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "ifanrx-react-ueditor": "^2.3.0",
    "lint-staged": "^10.0.7",
    "multer": "^1.4.2",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-ueditor-wrap": "^1.0.8",
    "typescript": "^3.8.3",
    "umi": "^3.0.14",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.27"
  }
}
